{
    "name": "Quick SaveAs",
    "version": "1.0",
    "description": "Quickly save as file using contextmenu shortcut",
    "manifest_version": 2,
    "icons": {

    },
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "background": {
        "scripts": [
          "background.js"
        ],
        "persistent": false
      },


      "browser_action": {
        "default_popup": "popup.html"
      }
  }